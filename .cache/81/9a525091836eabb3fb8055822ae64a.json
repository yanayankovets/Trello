ource":"scss/app.scss","name":null,"original":{"line":287,"column":2},"generated":{"line":264,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":288,"column":2},"generated":{"line":265,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":291,"column":0},"generated":{"line":268,"column":0}},{"source":"scss/app.scss","name":null,"original":{"line":292,"column":2},"generated":{"line":269,"column":2}}],"sources":{"scss/app.scss":"$purple: #CC99FF;\n$light-blue: #BEFFFF;\n$light-orange: #FFCC99;\n$light-green: #CCFF99;\n$gray: #787878;\n$charcoal: #404040;\n$light-gray: #CCCCCC;\n$white: #FFF;\n$light-blue-hover: #8ddde3;\n$light-green-hover: #a2e594;\n$background: rgba(0,0,0,0.4);\n$lavender: #9999FF;\n$pink: #FF9999;\n$grass: #bafbba;\n\nbody {\n  background-image: radial-gradient(rgb(153, 153, 153) 10%, transparent 10%);\n  background-position: 10px 10px;\n  background-size: 18px 18px;\n  background-color: rgb(255, 255, 255);\n}\n\n* {\n  border-radius: 5px;\n  font-family: Poppins, sans-serif;\n  color: $charcoal !important;\n}\n\n.board {\n  border: 1px solid $gray;\n  width: 1100px;\n  margin: 30px auto;\n  background-color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.header {\n  width: 1098px;\n  height: 100px;\n  background-color: $purple;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 50px;\n  font-size: 32px;\n}\n\n.time {\n  background-color: $white;\n  border: 1px solid $gray;\n  font-size: 18px;\n  width: 80px;\n  height: 42px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.todo-container {\n  display: flex;\n  justify-content: space-between;\n  margin: 20px 0;\n}\n\n.todo-column, .progress-column, .done-column {\n  border: 1px solid $gray;\n  width: 350px;\n  margin: 0 5px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  position: relative;\n}\n\n.todo-column__head, .progress-column__head, .done-column__head {\n  position: absolute;\n  top: 0;\n  width: 348px;\n  height: 50px;\n  border: 1px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 40px;\n\n}\n\n.todo-column__head {\n  background-color: $light-blue;\n}\n\n.progress-column__head {\n  background-color: $light-orange;\n}\n.done-column__head {\n  background-color: $light-green;\n}\n\n.todo-column__tasks, .progress-column_tasks, .done-column__tasks {\n  min-height: 370px;\n  overflow-y: scroll;\n}\n\n.btn:focus {\n  box-shadow: none !important;\n}\n\n.btn-primary, .btn-success {\n  position: absolute;\n  bottom: 0;\n  width: 348px;\n  height: 48px;\n  border: none !important;\n}\n\n.btn-primary {\n  background-color: $light-blue !important;\n}\n\n.btn-primary:hover {\n  background-color: $light-blue-hover !important;\n}\n\n.btn-success {\n  background-color: $light-green !important;\n}\n\n.btn-success:hover {\n  background-color: $light-green-hover !important;\n}\n\n\n//Modal\n\n.modal {\n  display: none;\n  position: fixed;\n  z-index: 1;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  overflow: auto;\n  background-color: $background;\n}\n\n.modal-content {\n  background-color: #fefefe;\n  margin: 100px auto;\n  border: 1px solid #888;\n  width: 500px !important;\n  height: 350px;\n  padding: 30px;\n}\n\n.title_input {\n  width: 260px;\n  height: 50px;\n  margin-bottom: 15px;\n  border: 1px solid $gray;\n  outline: none;\n  padding-left: 10px;\n}\n\n.description_input {\n  padding-left: 10px;\n  padding-top: 5px;\n}\n\ntextarea {\n  outline: none;\n}\n.modal_btns {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 20px;\n}\n.btn-danger, .btn-warning {\n  width: 85px;\n  border: none !important;\n}\n\n.btn-danger {\n  margin-left: 110px;\n  background-color: $pink !important;\n}\n.btn-warning {\n  background-color: $grass !important;\n}\n\n.btn-secondary {\n  background-color: $lavender !important;\n  border: none !important;\n  height: 40px;\n}\n\n\n// TODO Template\n\n.todo {\n  width: 330px;\n  min-height: 180px;\n  position: relative;\n  border: 1px solid $gray;\n  display: flex;\n  flex-direction: column;\n  padding: 12px;\n}\n\n.header-todo {\n  display: grid;\n  grid-template-columns: 3fr 1fr 1fr;\n  padding-bottom: 10px;\n}\n\n.title-todo {\n  margin-top: 10px;\n  font-size: 18px;\n  font-weight: bold;\n}\n\n.description-todo {\n  min-height: 50px;\n  word-wrap: break-word;\n  font-size: 16px;\n}\n\n.footer-todo {\n  display: flex;\n  justify-content: space-between;\n  padding-top: 10px;\n  font-size: 16px;\n}\n\n.user-todo, .time-todo {\n  font-size: 14px;\n}\n\n.btn-outline-secondary, .btn-outline-danger {\n  font-size: 14px !important;\n  height: 32px;\n  margin-left: 7px;\n}\n\n.btn-ou5line-secondary {\n  border: 1px solid $light-gray !important;\n}\n\n.btn-outline-secondary:hover {\n  background: $light-gray !important;\n}\n\n.btn-outline-danger {\n  border: 1px solid $pink !important;\n}\n\n.btn-outline-danger:hover {\n  background: $pink !important;\n}\n\n\n\n\n// TODO DRAG & DROP\n.todo {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  position: absolute;\n  width: 348px;\n  top: 50px;\n  background-color: #fff;\n  box-shadow: rgba(15, 15, 15, 0.1) 0 0 0 1px,\n  rgba(15, 15, 15, 0.1) 0 2px 4px;\n  border-radius: 4px;\n  padding: 0.5rem 1rem;\n  font-size: 1.5rem;\n  margin: 0.5rem 0;\n}\n\n.todo .close {\n  right: 1rem;\n  top: 0;\n  font-size: 2rem;\n  color: #ccc;\n  cursor: pointer;\n}\n\n.todo .close:hover {\n  color: #343444;\n}"},"lineCount":null}},"error":null,"hash":"0180dd9d1bc73504f1d55e853a1108ad","cacheData":{"env":{}}}
urce":"scss/app.scss","name":null,"original":{"line":327,"column":2},"generated":{"line":309,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":328,"column":2},"generated":{"line":310,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":329,"column":2},"generated":{"line":311,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":330,"column":2},"generated":{"line":312,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":331,"column":2},"generated":{"line":313,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":332,"column":2},"generated":{"line":314,"column":2}},{"source":"scss/app.scss","name":null,"original":{"line":1,"column":7},"generated":{"line":314,"column":20}},{"source":"scss/app.scss","name":null,"original":{"line":335,"column":0},"generated":{"line":317,"column":0}},{"source":"scss/app.scss","name":null,"original":{"line":336,"column":2},"generated":{"line":318,"column":2}}],"sources":{"scss/app.scss":"$pink: #FF9999;\n$purple: #CC99FF;\n$lavender: #9999FF;\n$light-blue: #BEFFFF;\n$light-blue-hover: #8ddde3;\n$light-orange: #FFCC99;\n$light-green: #CCFF99;\n$light-green-hover: #a2e594;\n$grass: #bafbba;\n$white: #FFF;\n$teknos-white: #fefefe;\n$light-gray: #CCCCCC;\n$gray: #787878;\n$charcoal: #404040;\n\n* {\n  box-sizing: border-box;\n  border-radius: 5px;\n  margin: 0;\n  font-family: Poppins, sans-serif;\n  color: $charcoal;\n}\n\nbody {\n  background-image: radial-gradient(rgb(153, 153, 153) 10%, transparent 10%);\n  background-position: 10px 10px;\n  background-size: 18px 18px;\n  background-color: rgb(255, 255, 255);\n}\n\n.container {\n  width: 1100px;\n  margin: 20px auto;\n  border: 1px solid $gray;\n  background-color: $white;\n}\n\n.header {\n  width: 1098px;\n  height: 95px;\n  background-color: $purple;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 80px;\n  margin-bottom: 10px;\n  font-size: 32px;\n}\n\n.clock {\n  background-color: $white;\n  border: 1px solid $gray;\n  font-size: 18px;\n  width: 80px;\n  height: 42px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.wrapper {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  background-color: $white;\n  padding: 20px;\n}\n\n/*Columns*/\n\n.column {\n  display: flex;\n  flex-direction: column;\n  border: 1px solid $gray;\n  width: 340px;\n  position: relative;\n}\n\n.column__cell {\n  min-height: 300px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background-color: $teknos-white;\n  margin-bottom: 50px;\n}\n\n.controls {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  justify-content: center;\n}\n\n.column__header, .add__card__btn, .delete__btn {\n  width: 338px;\n  height: 50px;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: center;\n  border: none;\n  font-size: 16px;\n}\n\n.add__card__btn, .delete__btn {\n  position: absolute;\n  bottom: 0;\n}\n\n.add__card__btn:hover {\n  background-color: $light-blue-hover;\n  cursor: pointer;\n}\n\n.delete__btn:hover {\n  background-color: $light-green-hover;\n  cursor: pointer;\n}\n\n.column__header.todo, .add__card__btn {\n  background-color: $light-blue;\n}\n\n.column__header.progress {\n  background-color: $light-orange;\n}\n\n.column__header.done, .delete__btn {\n  background-color: $light-green;\n}\n\n/*Modal*/\n\n.modal {\n  display: none;\n  position: fixed;\n  z-index: 1;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  overflow: auto;\n  background-color: rgb(0, 0, 0);\n  background-color: rgba(0, 0, 0, 0.4);\n}\n\n.modal-content {\n  background-color: $teknos-white;\n  margin: 115px auto;\n  padding: 30px;\n  border: 1px solid #888;\n  width: 450px;\n  height: 350px;\n}\n\n.title__input,.edit__title {\n  width: 260px;\n  height: 35px;\n  margin-bottom: 15px;\n  border: 1px solid $gray;\n  outline: none;\n  padding-left: 10px;\n}\n\n.description__input, .edit__description {\n  padding-left: 10px;\n  padding-top: 5px;\n}\n\ntextarea {\n  outline: none;\n  margin-bottom: 10px;\n  font-weight: 200;\n}\n\n.modal-footer {\n  display: flex;\n  justify-content: space-between;\n}\n\n.users__select {\n  margin-right: 55px;\n  outline: none;\n  padding: 0 10px;\n  width: 40%;\n  background-color: $lavender;\n}\n\n.users__select:hover {\n  cursor: pointer;\n}\n\n.close, .create__btn, .edit__btn {\n  width: 80px;\n  height: 40px;\n  border: none;\n  font-size: 14px;\n  font-weight: bold;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.close {\n  background-color: pink;\n}\n\n.create__btn, .edit__btn {\n  background-color: $grass;\n}\n\n.create__btn:hover {\n  cursor: pointer;\n}\n\n/*Card*/\n\n.card {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  width: 320px;\n  min-height: 150px;\n  border: 1px solid $gray;\n  padding: 10px;\n  margin: 7px 0;\n}\n\n.card:hover {\n  cursor: grab;\n}\n.card:active {\n  cursor: grabbing;\n}\n\n.card__controls {\n  position: absolute;\n  right: 3%;\n}\n\n.card__title, .card__description, .edit__description {\n  word-break: break-word;\n  line-height: 22px;\n  text-align: justify-all;\n}\n\n.card__title {\n  width: 160px;\n  padding-top: 7px;\n  min-height: 20px;\n  font-size: 16px;\n  margin-bottom: 10px;\n}\n\n.card__description {\n  width: 300px;\n  min-height: 20px;\n  margin-bottom: 20px;\n  font-weight: 200;\n  font-size: 15px;\n}\n\n.card__info {\n  position: absolute;\n  display: flex;\n  justify-content: space-between;\n  width: 94%;\n  bottom: 5px;\n  font-size: 14px;\n}\n\n.card__edit__btn, .card__delete__btn {\n  font-size: 14px;\n  height: 30px;\n  padding: 0 10px;\n  margin-left: 5px;\n}\n\n.card__edit__btn:hover, .card__delete__btn:hover, .edit__btn {\n  cursor: pointer;\n}\n\n.card__edit__btn {\n  border: 1px solid $gray;\n  background-color: #66CCFF;\n}\n\n.card__delete__btn {\n  border: 1px solid $gray;\n  background-color: $pink;\n}\n\n.close:hover, .close:focus {\n  text-decoration: none;\n  cursor: pointer;\n}\n\n.js-todo-cell .card {\n  background-color: $light-blue;\n}\n\n.js-in-progress-cell .card {\n  background-color:  $light-orange;\n}\n\n.js-done-cell .card {\n  background-color: $light-green;\n}\n\n/*Delete All*/\n\n.modal-content.delete {\n  background-color: $teknos-white;\n  margin: 115px auto;\n  padding: 30px;\n  border: 1px solid $gray;\n  width: 350px;\n  height: 200px;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  align-items: center;\n}\n\n.js-delete-all-btn {\n  width: 200px;\n  height: 40px;\n  margin-top: 20px;\n  border: 1px solid $gray;\n  outline: none;\n  background-color: $pink;\n}\n\n.js-delete-all-btn:hover {\n  cursor: pointer;\n}"},"lineCount":null}},"error":null,"hash":"2c44b307fcf3ec7439f7bd28262050d2","cacheData":{"env":{}}}